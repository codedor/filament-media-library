{
    "name": "wotz/filament-media-library",
    "description": "Media Library package for Filament",
    "keywords": [
        "who owns the zebra",
        "laravel",
        "filamentphp",
        "media",
        "attachments"
    ],
    "homepage": "https://github.com/wotzebra/filament-media-library",
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "blade-ui-kit/blade-icons": "^1.5",
        "wotz/filament-resource-picker": "^2.0",
        "wotz/filament-translatable-tabs": "^2.0",
        "filament/filament": "^5.0",
        "illuminate/contracts": "^10.0|^11.0|^12.0",
        "livewire/livewire": "^4.0",
        "owenvoke/blade-fontawesome": "^2.2",
        "spatie/image": "^3.0",
        "spatie/laravel-package-tools": "^1.12",
        "spatie/laravel-translatable": "^6.3"
    },
    "require-dev": {
        "larastan/larastan": "^2.0|^3.0",
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^7.0|^8.0",
        "orchestra/testbench": "^8.0|^9.0|^10.0",
        "pestphp/pest": "^2.0|^3.0|^4.0",
        "pestphp/pest-plugin-laravel": "^2.2|^3.0|^4.0",
        "phpstan/extension-installer": "^1.1|^2.0",
        "phpstan/phpstan-deprecation-rules": "^1.0|^2.0",
        "phpstan/phpstan-phpunit": "^1.0|^2.0"
    },
    "autoload": {
        "psr-4": {
            "Wotz\\MediaLibrary\\": "src",
            "Wotz\\MediaLibrary\\Database\\Factories\\": "database/factories"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Wotz\\MediaLibrary\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Wotz\\MediaLibrary\\Providers\\MediaLibraryServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
