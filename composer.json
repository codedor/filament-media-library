{
    "name": "codedor/filament-media-library",
    "description": "Media Library package for Filament",
    "keywords": [
        "codedor",
        "laravel",
        "filamentphp",
        "media",
        "attachments"
    ],
    "homepage": "https://github.com/codedor/filament-media-library",
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "blade-ui-kit/blade-icons": "^1.5",
        "codedor/filament-translatable-tabs": "^1.0",
        "codedor/filament-resource-picker": "^1.0",
        "filament/filament": "^3.0",
        "illuminate/contracts": "^10.0",
        "livewire/livewire": "^3.0",
        "owenvoke/blade-fontawesome": "^2.2",
        "spatie/image": "^2.2",
        "spatie/laravel-package-tools": "^1.12",
        "spatie/laravel-translatable": "^6.3"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^7.0",
        "nunomaduro/larastan": "^2.0.1",
        "orchestra/testbench": "^8.0",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Codedor\\MediaLibrary\\": "src",
            "Codedor\\MediaLibrary\\Database\\Factories\\": "database/factories"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Codedor\\MediaLibrary\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Codedor\\MediaLibrary\\Providers\\MediaLibraryServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
